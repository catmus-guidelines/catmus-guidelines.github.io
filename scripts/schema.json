{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "label": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "other-names": {
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "code": {
      "type": "string",
      "pattern": "^[0-9a-f]+$"
    },
    "position": {
      "type": "array",
      "items": [
        {
          "type": "string",
          "pattern": "^[A-I]$"
        }
      ]
    },
    "char": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "group": {
      "type": "string"
    },
    "corresp": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "transcription": {
            "type": "string"
          },
          "examples": {
            "type": "array",
            "items": {
              "type": "string",
              "pattern": "^[^\\s]+$"
            }
          },
          "name": {
            "type": "string"
          },
          "resolution": {
            "type": "string"
          },
          "code": {
            "type": "string",
            "pattern": "^[0-9a-f]+$"
          }
        },
        "required": [
          "transcription",
          "name",
          "code"
        ]
      }
    },
    "examples": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[^\\s]+$"
      }
    }
  },
  "required": [
    "label",
    "name",
    "other-names",
    "code",
    "char",
    "description",
    "group",
    "examples"
  ]
}